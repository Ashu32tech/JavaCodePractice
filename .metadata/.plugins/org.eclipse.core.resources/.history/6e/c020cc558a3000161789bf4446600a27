package dataStructure;

public class TestBubbleSort {
public static void main(String args[]){
	int arr[]={2,5,3,6,5};
	BubbleSort bs=new BubbleSort();
	bs.sortArr(arr);
	}	
}

class BubbleSort{
	public int [] sortArr(int [] arr){
	int i=0;
	int n=arr.length;
	for(int m=n;m>=0;m--){
		for(int j=0;j<=n-1;j++){
			int k=j+1;
			if(arr[i]>arr[j]){
				int temp=arr[i];
				arr[i]=arr[j];
				arr[j]=temp;
			}
			
			
		}
		
		
	}
	
	
	
		while(i<arr.length){
			System.out.println(arr[i]);
			i++;
		}
		return arr;
	}
}