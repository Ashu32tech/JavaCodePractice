package designPattern;

public interface Currency {
	String getSymbol();
}
class Rupee implements Currency{
	@Override
	public String getSymbol(){
		return "INR";
	}
}
class USDoller implements Currency{
	@Override
	public String getSymbol(){
		return "USD";
	}
}
class UKDoller implements Currency{
	@Override
	public String getSymbol(){
		return "UKD";
	}
} 

